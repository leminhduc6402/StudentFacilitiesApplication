{
  "expo": {
    "name": "StudentFacilitiesApp",
    "slug": "StudentFacilitiesApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.anvyidol.StudentFacilitiesApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "bce88816-ada4-4cb5-a4be-1b80b8e54cb6"
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./local/assets/bell.png",
          "color": "#ffffff"
        }
      ]
    ]
  }
}
